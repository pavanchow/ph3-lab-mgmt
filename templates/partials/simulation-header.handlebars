      <div class="dropdown align-self-stretch">
        <button class="navbar-toggler px-4 h-100"
              type="button" onclick="simulationHeaderToggle()">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="backdrop vlabs-hidden">
          <div class="nav-menu ">
            {{> nav_menu_items }}
          </div>
        </div>
      </div>
      <a href="https://vlab.co.in" class="navbar-brand">
        <img src="{{assets_path}}/assets/images/vlabs-color-small-moe.jpg" alt="vlabs logo" class="vlabs-logo">
      </a>
    </div>

    {{{experiment_name}}}

    <div class="expand-1">
      {{!-- For plugin buttons --}}
    </div>
</header>
